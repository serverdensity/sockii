{
   "name": "sockii",
   "version": "0.9.14",
   "description": "HTTP and WebSocket aggregation thinlayer",
   "author": {
       "name": "Server Density",
       "email": "hello@serverdensity.com",
       "web": "http://www.serverdensity.com/"
   }, 
   "repository": {
       "type": "git",
       "url": "git@github.com:serverdensity/sockii.git" 
   }, 
   "engines": {
       "node": ">= 0.8"
   },
   "bin": {
       "sockii": "./bin/sockii"
   },
   "dependencies": {
       "socket.io": "0.9.13",
       "ws": "0.4.25",
       "coffee-script": "1.3.3",
       "lodash": "1.2.0",
       "http-proxy": "0.10.2",
       "request": "2.20.0",
       "mongodb": "1.3.0",
       "socket.io-client": "0.9.11",
       "options": "*",
       "winston": "0.7.1",
       "longjohn": "0.2.0",
       "async": "0.1.22",
       "optimist": "0.4.0",
       "node-uuid": "1.4.0"
   },
   "devDependencies": {
       "vows": "0.7.0",
       "nock": "0.17.5"
   },
   "scripts": {
       "test": "vows tests/*-test.coffee --spec",
       "xunit-tests": "echo > tests.log && TESTOUTPUT=`vows tests/*-test.coffee --xunit` && cat ./tests.log && echo && mkdir -p ./test-reports && echo $TESTOUTPUT > ./test-reports/socky.xml"
   },
   "directories": {
       "lib": "src"
   },
   "main": "./src/run-sockii"
}
